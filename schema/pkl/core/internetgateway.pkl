module oci.core.internetgateway

import "@formae/formae.pkl"
import "../oci.pkl"

const type = "OCI::Core::InternetGateway"

open class InternetGatewayResolvable extends formae.Resolvable {
    hidden type = module.type

    hidden id: InternetGatewayResolvable = (this) {
        property = "Id"
    }
}

@oci.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = true
    extractable = true
    parent = "OCI::Core::VCN"
    listParam = List(
        new formae.ListProperty { parentProperty = "Id" listParameter = "VcnId" },
        new formae.ListProperty { parentProperty = "CompartmentId" listParameter = "CompartmentId" }
    )
}
open class InternetGateway extends formae.Resource {

    @oci.FieldHint{required = true createOnly = true}
    compartmentId: String|formae.Resolvable

    @oci.FieldHint{required = true createOnly = true}
    vcnId: String|formae.Resolvable

    @oci.FieldHint{required = true}
    isEnabled: Boolean

    @oci.FieldHint
    displayName: String?

    @oci.FieldHint {
        outputTransformation = oci.transform.tagsToFreeformTags
        outputField = "FreeformTags"
    }
    tags: Listing<formae.Tag>?

    @oci.FieldHint
    definedTags: Mapping<String, Mapping<String, Any>>?

    local parent = this

    hidden res: InternetGatewayResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}
