module oci.core.routetable

import "@formae/formae.pkl"
import "../oci.pkl"

const type = "OCI::Core::RouteTable"

open class RouteRule {
    @oci.FieldHint
    networkEntityId: (String|formae.Resolvable)?

    @oci.FieldHint
    destination: String?

    @oci.FieldHint
    destinationType: String?

    @oci.FieldHint
    description: String?
}

open class RouteTableResolvable extends formae.Resolvable {
    hidden type = module.type

    hidden id: RouteTableResolvable = (this) {
        property = "Id"
    }
}

@oci.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = true
    extractable = true
    parent = "OCI::Core::VCN"
    listParam = List(
        new formae.ListProperty { parentProperty = "Id" listParameter = "VcnId" },
        new formae.ListProperty { parentProperty = "CompartmentId" listParameter = "CompartmentId" }
    )
}
open class RouteTable extends formae.Resource {

    @oci.FieldHint{required = true createOnly = true}
    compartmentId: String|formae.Resolvable

    @oci.FieldHint{required = true createOnly = true}
    vcnId: String|formae.Resolvable

    @oci.FieldHint
    displayName: String?

    @oci.FieldHint
    routeRules: Listing<RouteRule>?

    @oci.FieldHint
    freeformTags: Listing<oci.FreeformTag>?

    @oci.FieldHint
    definedTags: Listing<oci.DefinedTag>?

    local parent = this

    hidden res: RouteTableResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}
