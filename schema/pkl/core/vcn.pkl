module oci.core.vcn

import "@formae/formae.pkl"
import "../oci.pkl"

const type = "OCI::Core::VCN"

open class VcnResolvable extends formae.Resolvable {
    hidden type = module.type

    hidden id: VcnResolvable = (this) {
        property = "Id"
    }
    hidden CompartmentId: VcnResolvable = (this) {
        property = "CompartmentId"
    }
    hidden cidrBlock: VcnResolvable = (this) {
        property = "CidrBlock"
    }
    hidden defaultDhcpOptionsId: VcnResolvable = (this) {
        property = "DefaultDhcpOptionsId"
    }
    hidden defaultRouteTableId: VcnResolvable = (this) {
        property = "DefaultRouteTableId"
    }
    hidden defaultSecurityListId: VcnResolvable = (this) {
        property = "DefaultSecurityListId"
    }
}

@oci.ResourceHint {
    type = module.type
    identifier = "Id"
    discoverable = true
    extractable = true
    parent = "OCI::Identity::Compartment"
    listParam = new formae.ListProperty {
        parentProperty = "Id"
        listParameter = "CompartmentId"
    }
}
open class VCN extends formae.Resource {

    @oci.FieldHint{required = true}
    compartmentId: String|formae.Resolvable

    @oci.FieldHint
    cidrBlock: String?

    @oci.FieldHint
    cidrBlocks: Listing<String>?

    @oci.FieldHint
    displayName: String?

    @oci.FieldHint
    dnsLabel: String?

    @oci.FieldHint
    isIpv6Enabled: Boolean?

    @oci.FieldHint
    freeformTags: Listing<oci.FreeformTag>?

    @oci.FieldHint
    definedTags: Listing<oci.DefinedTag>?

    local parent = this

    hidden res: VcnResolvable = new {
        label = parent.label
        stack = parent.stack?.label
    }
}
